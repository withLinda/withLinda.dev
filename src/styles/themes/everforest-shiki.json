  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#859289"
      }
    },
    {
      "scope": ["constant", "entity.name.constant", "variable.other.constant", "variable.other.enummember", "variable.language"],
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#D3C6AA"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#E69875"
      }
    },
    {
      "scope": ["storage.modifier.package", "storage.modifier.import", "storage.type.java"],
      "settings": {
        "foreground": "#D3C6AA"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string", "string punctuation.section.embedded source"],
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#DBBC7F"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#DBBC7F"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#D3C6AA"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#D3C6AA"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E67E80"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E67E80"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E67E80"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E67E80"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#E67E80",
        "foreground": "#2D353B",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#83C092"
      }
    },
    {
      "scope": ["string.regexp.character-class", "string.regexp constant.character.escape", "string.regexp source.ruby.embedded", "string.regexp string.regexp.arbitrary-repitition"],
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#DBBC7F"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#D699B6"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#DBBC7F"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#DBBC7F"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#E69875"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#DBBC7F"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#A7C080"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D3C6AA"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D3C6AA"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "scope": ["markup.deleted", "meta.diff.header.from-file", "punctuation.definition.deleted"],
      "settings": {
        "background": "#514045",
        "foreground": "#E67E80"
      }
    },
    {
      "scope": ["markup.inserted", "meta.diff.header.to-file", "punctuation.definition.inserted"],
      "settings": {
        "background": "#425047",
        "foreground": "#A7C080"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#3A515D",
        "foreground": "#7FBBB3"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#232A2E",
        "background": "#D3C6AA"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#D699B6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#7FBBB3"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7FBBB3"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#7FBBB3"
      }
    },
    {
      "scope": ["brackethighlighter.tag", "brackethighlighter.curly", "brackethighlighter.round", "brackethighlighter.square", "brackethighlighter.angle", "brackethighlighter.quote"],
      "settings": {
        "foreground": "#859289"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#E67E80"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#83C092"
      }
    }
